                  }}
                    {{input
                      type="text"
                      value=config.services.etcd.backupConfig.s3BackupConfig.region
                      classNames="form-control"
                      placeholder=(t "clusterNew.rke.etcd.backupConfig.region.placeholder")
                    }}
                  {{/input-or-display}}
                </CheckOverrideAllowed>
