<h2>
  {{t 'editPipelineConfig.header'}}
</h2>
<hr/>
<div>
  <div class="radio">
    <label>{{radio-button selection=push value=true}}&nbsp;{{t 'editPipelineConfig.pushToReop'}}</label>
  </div>
  <div class="radio">
    <label>{{radio-button selection=push value=false}}&nbsp;{{t 'editPipelineConfig.download'}}</label>
  </div>
  <div class="container-header-text display-name">
    {{#each branches as |branch|}}
        <div>{{t 'generic.branch'}}: {{branch}}</div>
    {{/each}}
  </div>
</div>

{{save-cancel createLabel="generic.confirm" save="save" cancel="cancel"}}