<button class="btn btn-primary btn-sm pull-right" style="position:relative;top:-30px;right:25px;" {{action 'expandChildren'}}>
  {{#if childrenExpanded}}
    <i class="fa fa-minus"></i> Collapse All
  {{else}}
    <i class="fa fa-plus"></i> Expand All
  {{/if}}
</button>
<a href="{{apiView}}" target="_blank" class="btn btn-primary btn-sm pull-right" style="position:relative;top:-30px;right:40px;">View In API</a>
<table class="grid fixed table table-hover">
  <thead>
    <th>Process Name</th>
    <th>Resource Type:Id</th>
    <th width="200">Exit Reason</th>
    <th width="150">Start Time</th>
    <th width="150">End Time</th>
    <th width="150">Run Time</th>
  </thead>
  <tbody>
    {{#each execution as |execution|}}
      {{process-execution-rows execution=execution topLevel=true expandAll=childrenExpanded}}
    {{/each}}
  </tbody>
</table>
