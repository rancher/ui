<a href="#" class="btn btn-sm btn-link" {{action "showAbout"}} aria-label="About Rancher">{{if settings.rancherVersion settings.rancherVersion '(Not a Release)'}}</a>

{{#unless settings.isPrivateLabel}}
  <a role="button" class="btn btn-sm btn-link" target="blank" href="{{docsBase}}">Documentation</a>
  <a role="button" class="btn btn-sm btn-link" target="blank" href="{{docsBase}}/faqs/">FAQs</a>
  <a role="button" class="btn btn-sm btn-link" target="blank" href="{{settings.issueUrl}}">File an Issue</a>
  <a role="button" class="btn btn-sm btn-link" target="blank" href="{{forumBase}}">Forums</a>
{{/unless}}
<div class="pull-right">
  <div class="compose-download inline-block">
    <a class="dropdown-toggle hand btn btn-sm btn-link" data-toggle="dropdown" aria-expanded="false">
      <i class="icon icon-download"></i> Download CLI <i class="icon icon-chevron-down"></i>
    </a>
    <ul class="dropdown-menu text-right" role="menu" style="min-width: 0">
      <li><a {{action "composeDownload" "darwin"}}>Mac OS X <i class="icon icon-fw icon-apple"></i></a></li>
      <li><a {{action "composeDownload" "windows"}}>Windows <i class="icon icon-fw icon-windows"></i></a></li>
      <li><a {{action "composeDownload" "linux"}}>Linux <i class="icon icon-fw icon-linux"></i></a></li>
    </ul>
  </div>
</div>
